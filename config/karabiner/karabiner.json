{
    "profiles": [
        {
            "complex_modifications": {
                "rules": [
                    {
                        "manipulators": [
                            {
                                "description": "Change left_control to lang2 and left_control.",
                                "from": {
                                    "key_code": "left_control",
                                    "modifiers": { "optional": ["any"] }
                                },
                                "to": [
                                    { "key_code": "lang2" },
                                    { "key_code": "left_control" }
                                ],
                                "to_if_alone": [{ "key_code": "left_control" }],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "manipulators": [
                            {
                                "description": "Ctrl+Cmd+h: send '/go-to-next-step' to ai-agent-session:1.1",
                                "from": {
                                    "key_code": "h",
                                    "modifiers": {
                                        "mandatory": ["control", "command"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "~/local/bin/tmux send-keys -t ai-agent-session:1.1 '/go-to-next-step' && sleep 0.1 && ~/local/bin/tmux send-keys -t ai-agent-session:1.1 Enter"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
                        "manipulators": [
                            {
                                "description": "Ctrl+Cmd+j: send '/act-on-tmp-prompt' to ai-agent-session:1.1",
