                                    "modifiers": {
                                        "mandatory": ["control", "command"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "osascript -e 'tell application \"Terminal\" to activate' && ~/local/bin/tmux select-window -t ai-agent-session:1 && ~/local/bin/tmux select-pane -t ai-agent-session:1.1"
