                                    "modifiers": {
                                        "mandatory": ["control", "command"]
                                    }
                                },
                                "to": [
                                    {
                                        "shell_command": "~/local/bin/tmux send-keys -t ai-agent-session:1.1 '/act-on-tmp-prompt' && sleep 0.1 && ~/local/bin/tmux send-keys -t ai-agent-session:1.1 Enter"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    },
                    {
